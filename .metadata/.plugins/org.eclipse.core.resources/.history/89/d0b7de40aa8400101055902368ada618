#include "stm32f4xx_cus_gpio.h"

void GPIO_INIT(GPIO_TypeDef *gpiox, uint8_t gpio_pins) {
	// Enable clock source

	// Pull-up or Pull-down

	// Handle input mode
	if (gpiox->MODER == GPIO_Input_Mode) {
		gpiox->MODER &= ~(GPIO_Moder_Mask << gpio_pins);
	}
// Handle output mode
	else if (gpiox->MODER == GPIO_Output_Mode) {
		gpiox->MODER = GPIO_Output_Mode;
	}
}

uint8_t GPIO_INPUT(GPIO_TypeDef *gpiox, uint8_t gpio_pins) {

}

void GPIO_OUTPUT(GPIO_TypeDef *gpiox, uint8_t gpio_pins, uint8_t val) {

}

