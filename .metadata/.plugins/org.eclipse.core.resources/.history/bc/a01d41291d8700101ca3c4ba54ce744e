#include "stm32f4xx_cus.h"

uint32_t Get_APB1_Clock_Speed() {
	uint8_t get_system_clock = (RCC->PLLCFGR >> 2) & 0x3;
	uint8_t get_ahb_pres = (RCC->PLLCFGR >> 4) & 0xF;
	uint8_t get_apb1_pres = (RCC->PLLCFGR >> 10) & 0x7;
	if (get_system_clock == 0) {
		// HSI System Clock

	} else if (get_system_clock == 1) {
		// // HSE System Clock
	} else if (get_system_clock == 2) {
		// PLL System Clock
	}
	return 0;
}
